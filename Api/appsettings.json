{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CacheOptions": {
    "ExpiresMinutes": "5"
  },
  "PersistanceAuthorizationOptions": {
    "RolePermissions": [
      {
        "Role": "Admin",
        "Permissions": [ "Read", "Write", "GetUsers" ]
      },
      {
        "Role": "User",
        "Permissions": [ "Read", "Write" ]
      }
    ]
  }
}
